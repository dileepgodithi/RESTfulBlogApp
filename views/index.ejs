<%- include('partials/header') %>
<h1 style="margin-top: 2em;">Index page!</h1>
<% blogs.forEach(function(blog){ %>
    <div>
        <h4><%= blog.title %></h4>
        <img src="<%= blog.image %>">
        <p><%= blog.body %></p>
        <p><%= blog.created %></p>
    </div>
<% }) %>
<%- include('partials/footer') %>