<%- include('partials/header') %>
<h1 style="margin-top: 2em;">Index page!</h1>
<% blogs.forEach(function(blog){ %>
    <div>
        <h4><%= blog.title %></h4>
        <img src="<%= blog.image %>">
        <p><%- blog.body.substring(0,100) %>...</p>
        <p><%= blog.created %></p>
        <a href="/blogs/<%=blog._id %>">Show more</a>
    </div>
<% }) %>
<%- include('partials/footer') %>